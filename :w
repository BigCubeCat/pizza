import React from 'react';
import styles from './OrderButton.module.css';
import {
  Button, Typography
} from '@mui/material';

export default function OrderButton({ text }: { text: string }) {
  return (
    <Button
      variant="contained"
      sx={{borderRadius: 15, paddingLeft: 5, paddingRight: 5}}
    >
      <Typography style={{
        fontSize: 16,
        fontWeight: "bold",
      }}>{text}</Typography>
    </Button>
  );
}
